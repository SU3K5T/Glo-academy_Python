for var i := 0 to a.Cnt-2 do
  for var j := i+1 to a.Cnt-1 do 
  begin
    f := (a[i] + a[j])/2;
    var k := a.Wh(x â†’ x<f).Cnt;
    if (k<>0) and (k mod 100=0) then 
      (mx,kk) := (max(mx,k),kk+1)
  end;
  print(mx,kk)
