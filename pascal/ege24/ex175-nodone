uses school;
var a := ReadAllText('\\Mac\Home\Downloads\ege2022kp\24data\24-175.txt');
var (z,h,k,max) := (0,0,0,int.MinValue);
for var i := 1 to a.Cnt -4 do 
begin 
  z += 1;
  if (a[i+1] = 'K') and (a[i+2] = 'E') and (a[i+3] = 'G') and (a[i+4] = 'E') then
  begin
    k += 1;
  end;
  if k > 2 then 
  begin
    if z > max then 
      max := z;
    k := 0;
    z := 0;
end;
end;
print(max+9)
