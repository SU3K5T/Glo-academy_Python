
var z:integer;
function F(n: integer): integer;
var d:integer;
begin
 z:=n;
 //writeln(N);
 if n > 0 then begin
   d := n mod 10+F(n div 10);
   z:=d;
 // writeln(d);
   F := d 
 end
 else F:= 0; 
end;
begin
for var i:=1 to 100000 do
  begin 
 F(i);
 if (z > 32) then
    println(F(i), i);
  
  end;
  end.
