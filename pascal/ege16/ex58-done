###
uses school;
var a:array [1..1000] of integer;
var k := 0;
for var i := 1000 downto 1 do 
begin
  if i > 30 then 
    a[i] := i * i + 3 * i +5;
  if (i <= 30) and (i mod 2 = 0) then 
    a[i] := 2 * a[i+1] + a[i+4];
  if (i <= 30) and (i mod 2 <> 0) then 
    a[i] := a[i+2] + 3 * a[i + 5];
  if digits(a[i]).Wh(x â†’ x=0).Cnt>=2 then
    k +=1;
end;
pr(k)
