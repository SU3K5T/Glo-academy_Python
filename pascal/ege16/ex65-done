uses school;
var a:array [1..1000] of int64;
var k := 0;
for var i := 1 to 1000 do 
begin
  if i <= 5 then 
    a[i] := i + 15;
  if (i > 5) and (i mod 2 = 0) then 
    a[i] :=a[i div 2] + i * i * i - 1;
  if (i > 5) and (i mod 2 <> 0) then 
    a[i] := a[i-1] + 2 * i * i + 1;
  if digits(a[i]).Wh(x â†’ x=8).Cnt>=2 then 
    k += 1
end;
pr(k)
