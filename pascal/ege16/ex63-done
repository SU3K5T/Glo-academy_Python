uses school;
var a:array [1..1000] of int64;
var k := 0;
for var i := 1 to 1000 do 
begin
  var flag := 0;
  if i <= 13 then 
    a[i] := i *i * i + i * i + 1;
  if (i > 13) and (i mod 3 = 0) then 
    a[i] := a[i-1] +2 * i *i - 3;
  if (i > 13) and (i mod 3 <> 0) then 
    a[i] := a[i-2] +3 *i + 6;
    if (digits(a[i]).Wh(x â†’ x.iseven).Cnt=0) then
      k+=1;
end;
pr(k)
