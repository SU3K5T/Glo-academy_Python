###
uses school;
var a:array [1..1000] of int64;
var k := 0;
for var i := 1 to 1000 do 
begin
  var flag := 0;
  if i <= 15 then 
    a[i] := i * i + 3 * i + 9;
  if (i > 15) and (i mod 3 = 0) then 
    a[i] := a[i-1] + i - 2;
  if (i > 15) and (i mod 3 <> 0) then 
    a[i] := a[i-2] + i + 2;
  if digits(a[i]).Wh(x â†’ x.isodd).Cnt=0 then
   k += 1;
end;
pr(k)
