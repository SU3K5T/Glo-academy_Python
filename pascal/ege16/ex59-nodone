###
uses school;
var a:array [1..1000] of int64;
var k := 0;
for var i := 1000 downto 1 do 
begin
  if i > 30 then 
    a[i] := i * i + 5 * i + 4;
  if (i <= 30) and (a[i] mod 2 = 0) then 
    a[i] := a[i+1] + 3 * a[i+4];
  if (i <= 30) and (a[i] mod 2 <> 0) then 
    a[i] := 2 * a[i+2] + a[i+5];
  if (a[i] >0 ) and ((digits(a[i]).sum) = 27) then
   k += 1;
end;
pr(k)
