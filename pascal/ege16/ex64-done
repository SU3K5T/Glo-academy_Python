###
uses school;
var a:array [1..800] of int64;
var k := 0;
for var i := 1 to 800 do 
begin
  if i <= 18 then 
    a[i] := i + 3;
  if (i > 18) and (i mod 3 = 0) then 
    a[i] := (i div 3) * a[i div 3] + i - 12;
  if (i > 18) and (i mod 3 <> 0) then 
    a[i] := a[i-1] +i * i + 5;
  if (digits(a[i]).Wh(x â†’ x.isodd).Cnt=0) then
    k += 1;
end;
pr(k)
