var count, max, i:int64;
begin
    count := 0;
    for i:= 1234567 to 7654321 do 
        begin

            
           
           
            if i mod ((i div 100000) - (i mod 100)) = 0 then
            begin
               max := i;
               count := count + 1;
            end;
               
           
        end;
    writeln(count, ' ', max);
end.


