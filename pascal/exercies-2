var count, i, max, m, x,  z:int64;
begin
    count := 0;

    for i:= 1388 to 63252 do 
        begin
            m := i;
            x := 0;

            z := 0;
            while m > 0 do
            begin
              if m mod 10 = 7 then
                z := 1; 
              if m mod 10 = 4 then
                x := 1;
            
              
              m := m div 10;

            end;
                      if (i mod 12 <> 0) and ((x  = 1) or (z = 1)) then
                      begin

                             max := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', max);
end.

