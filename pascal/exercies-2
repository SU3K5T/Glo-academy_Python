begin
    count := 0;
    s := 0;
    for i:= 2595 to 8401 do 
        begin



                      if (i mod 2 = 0) and (i mod 13 <> 2) then
                      begin
                        s := i + s;
                        count := count + 1;
                      end;
                        

                    
        end;
    writeln(count, ' ', s);
end.

