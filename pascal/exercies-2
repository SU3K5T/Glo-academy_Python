=================================================================4.  -----косяк

var count, max, i, x, m, k, h:int64;
begin
    count := 0;

    for i:= 1000 to 70000 do 
        begin
            m := i;
            x := 0;
            h := 0;
            k := i;
            while m > 0 do 
                begin
                  m := m div 8;
                  x := x + 1;
                end;
            while k > 0 do
                begin
                  k := k div 5;
                  h := h + 1;
                end;

            if (x = 5) and (h = 6) and ((i div 16 = 0)mod 16 = $F) and (i mod 16 = $A) then
                    begin
                      count := count + 1;
                      max := i;
                    end;
                    
        end;
    writeln(count, ' ', max);
end.

