var count, i, max, m, flag:int64;
begin
    count := 0;
    
    for i:= 333666 to 666999 do 
        begin
        m := i;
        flag := 0;
        while m > 0 do 
            begin
               
                if m mod 10 = 7 then 
                    flag := flag + 1;
                m := m div 10;
                
            end;
                      if (i mod 17 = 0) and (flag = 2) then
                      begin
                        max := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', max);
end.

