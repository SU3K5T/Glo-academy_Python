var count, i, max:int64;
begin
    count := 0;
    
    for i:= 100001 to 900009 do 
        begin
  
                      if ((i mod 7 + (i div 10 mod 10) + i mod 10) = 10) and (i mod 11 = 0) and (i mod 55 <> 0)then
                      begin
                        max := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', max);
end.


======================= 20

var count, i, min, m, x, y, z:int64;
begin
    count := 0;
    min := 50000;
    for i:= 2079 to 43167 do 
        begin
            m := i;
            x := 0;
            y := 0;
            z := 0;
            while m > 0 do
            begin
              if m mod 10 = 0 then
                z := 1; 
              if m mod 10 = 2 then
                x := 1;
              if m mod 10 = 5 then
                y := 1;
              
              m := m div 10;

            end;
                      if (i mod 7  = 0) and (x  = 1) and (y = 1) and (z = 1)then
                      begin
                        if i < min then
                             min := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', min);
end.

