var count, max, i, min, s:int64;
begin
    min := 10000;
    s := 0;
    for i:= 10 to 1178 do 
        begin

            if i mod 2 = 0 then
                    begin
                      if (i mod 10 <> 0) and (i mod 10 <> 2) and (i mod 10 <> 6) and (i mod 10 <> 8) and (i mod 100 <> 14) then
                      begin
                        s := i + s;
                        if i < min then
                            min := i;
                      end;
                        
                    end;
                    
        end;
    writeln(min, ' ', s);
end.


