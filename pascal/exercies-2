----============================================================--1

var count, max, i, x, m, k, h:integer;
begin
    count := 0;

    for i:= 127 to 9852 do 
        begin
            m := i;
            x := 0;
            h := 0;
            k := i;
            while m > 0 do 
                begin
                  m := m div 10;
                  x := x + 1;
                end;
            while k > 0 do
                begin
                  k := k div 8;
                  h := h + 1;
                end;

            if (h = x) and (i mod 3 = 0) and (i mod 9 <> 0) then
                    begin
                      count := count + 1;
                      max := i;
                    end;
                    
        end;
    writeln(count, ' ', max);
end.


===========================================================================================================2

var count, min, i, x, m, k, h:integer;
begin
    count := 0;
    min := 10000;
    for i:= 331 to 8751 do 
        begin
            m := i;
            x := 0;
            h := 0;
            k := i;
            while m > 0 do 
                begin
                  m := m div 10;
                  x := x + 1;
                end;
            while k > 0 do
                begin
                  k := k div 16;
                  h := h + 1;
                end;

            if (h = x) and (i mod 5 = 0) and (i mod 25 <> 0) then
                    begin
                      count := count + 1;
                      if i < min then 
                        min := i;
                    end;
                    
        end;
    writeln(count, ' ', min);
end.

