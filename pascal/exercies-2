var count, i, max:int64;
begin
    count := 0;

    for i:= 4563 to 7912 do 
        begin

                      if (i mod 7 = 0) and ((i div 1000 + i mod 10) > 10) then
                      begin
                        max := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', max);
end.

===========================17

var count, i, max, pr, m, flag:int64;
begin
    count := 0;
    
    for i:= 8800 to 55535 do 
        begin
        m := i;
        pr := 1;
        flag := 0;
        while m > 0 do 
            begin
                pr := pr * (m mod 10);
                if m mod 10 = 7 then 
                    flag := 1;
                m := m div 10;
                
            end;
                      if (pr > 35) and (flag = 1) then
                      begin
                        max := i;
                        count := count + 1;
                      end;
        end;
    writeln(count, ' ', max);
end.

