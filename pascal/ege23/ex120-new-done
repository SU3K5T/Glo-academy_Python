uses school, SF;
var a:set Of Int;
var k := 0;
function F(x,l:int):int;
begin
  if (x<l) then
  begin
    f(x+1,l);
    f(x+5,l);
    f(x*3,l)
  end else 
    if (x=l) then
     k += 1;
end;
begin 
 for var i:=1 to 1000 do
   begin
     k := 0;
    F(1,i);
      if k = 175 then 
     print(i)
    end;
end.
