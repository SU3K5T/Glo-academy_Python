var count, i, max:int64;
begin
    count := 0;
    for i:= 100 to 10000 do 
        begin
          
            if (i mod 10 = 3) and (i mod 8 = 7)    and (i mod 13 <> 0)    and (i mod 16 <> 0)    and (i mod 19 <> 0) and (i mod 21 = 0)  then
            begin
               max := i;
               count := count + 1;
            end;
               
           
        end;
    writeln(count, ' ', max);
end.


