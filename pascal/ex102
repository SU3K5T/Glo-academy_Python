var count, i, max:int64;
begin
    count := 0;
    for i:= 15 to 1000 do 
        begin
          
            if (i mod 7 = 6) and (i mod 32 = 0) then
            begin
               max := i;
               count := count + 1;
            end;
               
           
        end;
    writeln(count, ' ', max);
end.


