uses school;
var s := 0.0;
var m := -maxreal;
var a := ReadAllLines('\\Mac\Home\Downloads\ege2022kp\18data\18-77.csv').Sel(x â†’ x.replace(',','.').ToR).ToA;
for var i := 0 to a.Cnt-2 do
begin
  if abs(a[i]-a[i+1]) <= 8 then 
    s += a[i]
  else 
  begin
    m := max(s+a[i],m);
    s := 0.0;
  end;
end;
trunc(m).pr
