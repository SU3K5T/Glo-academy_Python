###
uses school;
var max := 0; var kol:=0;
for var i := 394480 to 394540 do
begin
  var k := 2; var s := 0;
  for var j := 2 to i div 2 do 
    if i mod j = 0 then
    begin
      k += 1;
      s := j;
    end;
    if k = 48 then
      begin
       kol += 1;
      println(kol,48,i,s)
      end;
end;

//48
