###
uses school;
var k := 0;
var kol := 0;
var min := 89889898;
for var i:= 2 to 20000 do 
begin
  k := 0;
  for var j := 1 to i div 2 do
    if (i mod j = 0) and j.isPrime then
    begin
      k += 1;
    end;
    if k = 5 then
      begin
         if i < min then 
        min := i;
       println(min,k);
       break
       end;
end;

