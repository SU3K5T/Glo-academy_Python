###
uses school;
var kol := 0;
var x := 0;
for var i := 194441 to 196500 do 
begin
  var k := 2;
  var s := 0;
  for var j := 2 to i div 2 do 
    if i mod j = 0 then
      begin
        k += 1;
        s := j;
        if (trunc(sqrt(i)) = j) then
          begin
          x := j;
           end;    
      end;
      if k mod 2 <> 0 then
      begin
        kol += 1;
        
        println(kol,i,k,x)
      end;
end;
