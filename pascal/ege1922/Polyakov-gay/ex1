//19
###
uses school;
[cache]
function H(x:int):int ;
begin
  if (x >= 68) then H := 0 else 
  begin
    var a := |H(x+1), H(x*2)|;
  if (a.Cnt(0)>0) then H:=1 else 
  if a.Wh(a → (a<0)).Cnt>0 then H:=a.Wh(a → (a<0)).Max*(-1)+1 else 
          H := a.Max*(-1)
  end;
end;
(1..67).Wh(i → (H(i+1) = 1) or (H(i*2) = 1)).min.Prln;
