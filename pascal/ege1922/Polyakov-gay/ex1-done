###
uses school;
[cache]
function H(x,y:int):int ;
begin
  if (x+y >= 75) then H := 0 else 
  begin
    var a := |H(x+1,y), H(x*2,y), H(x,y*2), H(x,y+1)|;
  if (a.Cnt(0)>0) then H:=1 else 
  if a.Wh(a → (a<0)).Cnt>0 then H:=a.Wh(a → (a<0)).Max*(-1)+1 else 
          H := a.Max*(-1)
  end;
end;
(1..66).Wh(i → (H(8,i) = -2)).min.Prln;
