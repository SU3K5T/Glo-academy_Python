var count, i, max:int64;
begin
    count := 0;
    for i:= 99 to 998 do 
        begin
          
            if (i mod 10 = 9) and (i mod 8 = 1) and (i mod 18 <> 0) then
            begin
               max := i;
               count := count + 1;
            end;
               
           
        end;
    writeln(count, ' ', max);
end.


