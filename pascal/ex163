var a := ReadLines('\\Mac\Home\Desktop\помойка\да ну нахуй\17data\17-3.txt').Select(t -> t.ToInteger).ToArray;

var count := 0;
var max := -2000000;
var s := 0;
for var i := 0 to a.Cnt-2 do 
begin
 var flag := 0;
 var flag2 := 0;
 if a[i] or a[i + 1] mod 2 = 0 then
   flag := 1;
 if a[i] mod 11 = 0 then
   flag2 := 1;
 
 if (flag = 1) and (flag2 = 1) then
 begin
   if a[]
   s := s + a[i] + a[i + 1];
   if s > max then 
     max := s;
 end;
  
end;

print(max, count)
