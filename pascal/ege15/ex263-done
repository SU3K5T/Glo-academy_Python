uses school;
var k := 0;
for var a := 1 to 1000 do
begin
  var f := true;
  for var x := 1 to 1000 do
    for var y := 1 to 1000 do
      if ( (y*y < 30) <= (y < A) ) and ( (x <= A) <= (x*x < 150) ) = false then 
      begin
        f := false;
        break
      end;
      if f then
        k += 1;
end;
pr(k)
