uses school;
var k := 0;
for var a := 1 to 1000 do
begin
  var f:= true;
  for var x := 1 to 1000 do
    for var y := 1 to 1000 do
      if not((x > 10) or (x* x < A)) or not((y*y >=  A) or (y <= 10)) then
        begin
        f := false;
