
###
uses school;
var p := range(10,50);
var q := range(15,20);
var r := range(30,80);
var a := range(25, 50);
var f := true;
for var x := 1 to 1000 do
  if (( (x in p) <= (x in q) ) or ( not(x in a) <= not(x in r) )) = false then
  begin
    f := false;
    break
  end;
  if f then
    pr(1)
  else 
    pr(0)
