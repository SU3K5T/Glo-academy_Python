 uses school;
 for var a := 1 to 1000 do
 begin
   var flag := True;
   var flag2 := True;
   for var x := 1 to 1000 do
     if ( (x <= 9) <= (x*x <= A) ) = false then
     begin
       flag := False;
       break
     end;
     for var y := 1 to 1000 do
       if ( (y*y <= A) <= (y <= 10) ) = false then
       begin
         flag2 := False;
         break
       end;
     if flag and flag2 then
      print(a)
 end;
