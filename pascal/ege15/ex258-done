uses school;
var k:=0;
for var a := 1 to 1000 do
  begin
  var f := true;
  for var x := 1 to 1000 do
    for var y := 1 to 1000 do
         if ( (x >= 11) <= (x*x+2*x > A) ) and ( (y*y + 3*y >= A) <= (y > 8) ) = false then
         begin
           f := false;
          break
         end;
       if f then
         k += 1;
       end;
       pr(k)
