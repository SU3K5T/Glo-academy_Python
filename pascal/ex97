var count, i, s, m:int64;
begin
    count := 0;
    s := 0;
    for i:= 777 to 3777 do 
        begin
           m := abs(i);
            if i < 1000
                 m : = i div 16
            if (m mod 16 = $F) and (m mod 11 <> 0) ) then
            begin
               s := s + i;
               count := count + 1;
            end;
               
           
        end;
    writeln(count, ' ', s);
end.


